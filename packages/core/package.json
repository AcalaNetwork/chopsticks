{
	"name": "@acala-network/chopsticks-core",
	"version": "1.0.4",
	"author": "Acala Developers <hello@acala.network>",
	"license": "Apache-2.0",
	"type": "module",
	"engines": {
		"node": ">=v20"
	},
	"scripts": {
		"clean": "rm -rf dist tsconfig.tsbuildinfo",
		"build": "yarn clean && ../../package-build.sh",
		"docs:prep": "typedoc",
		"depcheck": "npx depcheck"
	},
	"dependencies": {
		"@acala-network/chopsticks-executor": "workspace:*",
		"@polkadot/rpc-provider": "^15.7.1",
		"@polkadot/types": "^15.7.1",
		"@polkadot/types-codec": "^15.7.1",
		"@polkadot/types-known": "^15.7.1",
		"@polkadot/util": "^13.4.3",
		"@polkadot/util-crypto": "^13.4.3",
		"comlink": "^4.4.2",
		"eventemitter3": "^5.0.1",
		"lodash": "^4.17.21",
		"lru-cache": "^11.0.2",
		"pino": "^9.6.0",
		"pino-pretty": "^13.0.0",
		"rxjs": "^7.8.2",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@swc/cli": "0.6.0",
		"@swc/core": "^1.11.1",
		"@types/lodash": "^4.17.15",
		"typescript": "^5.7.3",
		"vitest": "^3.0.7"
	},
	"files": [
		"dist/esm/**",
		"dist/cjs/**"
	],
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"exports": {
		".": {
			"require": "./dist/cjs/index.js",
			"import": "./dist/esm/index.js",
			"default": "./dist/esm/index.js"
		},
		"./*": {
			"require": "./dist/cjs/*.js",
			"import": "./dist/esm/*.js",
			"default": "./dist/esm/*.js"
		}
	},
	"browser": {
		"./dist/cjs/wasm-executor/node-worker.js": "./dist/cjs/wasm-executor/browser-worker.js",
		"./dist/esm/wasm-executor/node-worker.js": "./dist/esm/wasm-executor/browser-worker.js"
	}
}
