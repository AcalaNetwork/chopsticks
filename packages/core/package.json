{
	"name": "@acala-network/chopsticks-core",
	"version": "1.2.3",
	"author": "Acala Developers <hello@acala.network>",
	"license": "Apache-2.0",
	"type": "module",
	"engines": {
		"node": ">=v22"
	},
	"scripts": {
		"clean": "rm -rf dist tsconfig.tsbuildinfo",
		"build": "yarn clean && ../../package-build.sh",
		"docs:prep": "typedoc",
		"depcheck": "npx depcheck"
	},
	"dependencies": {
		"@acala-network/chopsticks-executor": "workspace:*",
		"@polkadot/rpc-provider": "^16.4.9",
		"@polkadot/types": "^16.4.9",
		"@polkadot/types-codec": "^16.4.9",
		"@polkadot/types-known": "^16.4.9",
		"@polkadot/util": "^13.5.7",
		"@polkadot/util-crypto": "^13.5.7",
		"comlink": "^4.4.2",
		"eventemitter3": "^5.0.1",
		"lodash": "^4.17.21",
		"lru-cache": "^11.2.2",
		"pino": "^10.1.0",
		"pino-pretty": "^13.1.2",
		"rxjs": "^7.8.2",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"@swc/cli": "0.7.8",
		"@swc/core": "1.13.5",
		"@types/lodash": "^4.17.20",
		"typescript": "^5.9.3",
		"vitest": "^4.0.2"
	},
	"files": [
		"dist/esm/**",
		"dist/cjs/**"
	],
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"exports": {
		".": {
			"require": "./dist/cjs/index.js",
			"import": "./dist/esm/index.js",
			"default": "./dist/esm/index.js"
		},
		"./*": {
			"require": "./dist/cjs/*.js",
			"import": "./dist/esm/*.js",
			"default": "./dist/esm/*.js"
		}
	},
	"browser": {
		"./dist/cjs/wasm-executor/node-worker.js": "./dist/cjs/wasm-executor/browser-worker.js",
		"./dist/esm/wasm-executor/node-worker.js": "./dist/esm/wasm-executor/browser-worker.js"
	}
}
